{
  "name": "ts-bus-operation-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview --host 127.0.0.1 --port 4173 --strictPort",
    "test": "tsx --test tests/**/*.test.ts",
    "test:visual": "tsx tools/ui-snapshots/runWithPreview.ts",
    "snapshots:update": "tsx tools/ui-snapshots/runWithPreview.ts --update",
    "snapshots:install": "playwright install chromium",
    "generate-snapshots": "npm run build && npm run snapshots:install && npm run test:visual",
    "approve-baseline": "npm run snapshots:update",
    "context7:docs": "tsx tools/mcps/context7/docsCli.ts",
    "context7:resolve": "tsx tools/mcps/context7/resolveCli.ts",
    "scan:encoding": "tsx tools/encodingScanCli.ts",
    "smoke:chrome": "tsx tools/chromeSmoke.ts",
    "smoke:ui": "tsx tools/uiSmoke.ts",
    "devtools:landing-hero": "tsx tools/devtools/landingHeroCheck.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.544.0",
    "maplibre-gl": "^5.8.0",
    "papaparse": "^5.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/chrome-remote-interface": "^0.31.14",
    "@types/node": "^22.14.0",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "chrome-remote-interface": "^0.33.3",
    "playwright": "^1.56.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.20.6",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
